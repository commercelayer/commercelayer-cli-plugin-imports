{
  "name": "@commercelayer/cli-plugin-imports",
  "description": "Commerce Layer CLI Imports plugin",
  "version": "3.1.6",
  "author": "Pierluigi Viti <pierluigi@commercelayer.io>",
  "bugs": "https://github.com/commercelayer/commercelayer-cli-plugin-imports/issues",
  "devDependencies": {
    "@commercelayer/cli-dev": "^0.1.6",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/test": "^2.1.0",
    "@types/chai": "^4.3.0",
    "@types/cli-progress": "^3.9.2",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "chai": "^4.3.6",
    "eslint": "^8.23.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/commercelayer/commercelayer-cli-plugin-imports",
  "keywords": [
    "oclif",
    "ecommerce",
    "cli",
    "commercelayer"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cl-imports",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "plugins": [],
    "topics": {
      "imports": {
        "description": "Import resources in Commerce Layer",
        "hidden": false
      }
    },
    "repositoryPrefix": "<%- repo %>/blob/main/<%- commandPath %>",
    "additionalHelpFlags": [
      "-h"
    ],
    "topicSeparator": ":"
  },
  "repository": "commercelayer/commercelayer-cli-plugin-imports",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif manifest && npm run readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "readme": "cl-cli-dev readme --plugin --bin=commercelayer && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@commercelayer/cli-core": "^1.3.13",
    "@commercelayer/js-auth": "^2.3.0",
    "@commercelayer/sdk": "^4.10.0",
    "@fast-csv/parse": "^4.3.6",
    "@oclif/core": "^1.6.1",
    "cli-progress": "^3.10.0",
    "cli-table3": "0.6.2",
    "tslib": "^2.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
