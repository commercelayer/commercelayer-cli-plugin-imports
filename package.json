{
  "name": "@commercelayer/cli-plugin-imports",
  "description": "Commerce Layer CLI Imports plugin",
  "version": "3.2.1",
  "author": "Pierluigi Viti <pierluigi@commercelayer.io>",
  "bugs": "https://github.com/commercelayer/commercelayer-cli-plugin-imports/issues",
  "engines": {
    "node": ">=16"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/commercelayer/commercelayer-cli-plugin-imports",
  "keywords": [
    "oclif",
    "ecommerce",
    "cli",
    "commercelayer"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cl-imports",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "plugins": [],
    "topics": {
      "imports": {
        "description": "Import resources in Commerce Layer",
        "hidden": false
      }
    },
    "repositoryPrefix": "<%- repo %>/blob/main/<%- commandPath %>",
    "additionalHelpFlags": [
      "-h"
    ],
    "topicSeparator": ":"
  },
  "repository": "commercelayer/commercelayer-cli-plugin-imports",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "build": "rm -rf lib && tsc -b",
    "prepack": "npm run build && oclif manifest && npm run readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "readme": "cl-cli-dev readme --plugin --bin=commercelayer && git add README.md",
    "lint": "eslint src --ext .ts --config .eslintrc",
    "lint:fix": "eslint src --fix"
  },
  "types": "lib/index.d.ts",
  "devDependencies": {
    "@commercelayer/cli-dev": "^0.1.10",
    "@commercelayer/eslint-config-ts": "^0.1.4",
    "@oclif/plugin-help": "^5.1.19",
    "@oclif/test": "^2.2.10",
    "@types/chai": "^4.3.4",
    "@types/cli-progress": "^3.11.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "chai": "^4.3.7",
    "eslint": "^8.27.0",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@commercelayer/cli-core": "^2.0.0",
    "@commercelayer/js-auth": "^2.3.0",
    "@commercelayer/sdk": "^4.17.0",
    "@fast-csv/parse": "^4.3.6",
    "@oclif/core": "^1.20.4",
    "cli-progress": "^3.11.2",
    "cli-table3": "^0.6.3",
    "tslib": "^2.4.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
